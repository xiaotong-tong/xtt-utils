{
	"name": "xtt-utils",
	"version": "0.0.0",
	"description": "typescript & javascript utils",
	"type": "module",
	"main": "index.js",
	"scripts": {
		"postinstall": "rollup -c rollup.config.js",
		"pretest": "rollup -c rollup.config.js",
		"test": "jest",
		"prebuild": "rimraf dist",
		"build": "rollup -c rollup.config.js"
	},
	"exports": {
		"./*": {
			"import": "./dist/esm/*.js",
			"require": "./dist/cjs/*.cjs",
			"types": "./dist/types/*.d.ts"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xiaotong-tong/xtt-utils.git"
	},
	"keywords": [
		"utils",
		"typescript",
		"javascript"
	],
	"author": "xtt",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xiaotong-tong/xtt-utils/issues"
	},
	"homepage": "https://github.com/xiaotong-tong/xtt-utils#readme",
	"devDependencies": {
		"@rollup/plugin-typescript": "^11.0.0",
		"jest": "^29.5.0",
		"rimraf": "^4.4.0",
		"rollup": "^3.19.1",
		"tslib": "^2.5.0"
	}
}
